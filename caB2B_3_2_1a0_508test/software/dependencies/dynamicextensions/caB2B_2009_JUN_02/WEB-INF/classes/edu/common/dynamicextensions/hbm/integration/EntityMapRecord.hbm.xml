<?xml version='1.0'?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class
        name="edu.common.dynamicextensions.domain.integration.EntityMapRecord"
        table="DYEXTN_ENTITY_MAP_RECORD"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">DYEXTN_ENTITY_RECORD_SEQ</param>
            </generator>
        </id>

        <many-to-one
            name="formContext"
            class="edu.common.dynamicextensions.domain.integration.FormContext"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="FORM_CONTEXT_ID"
        />

        <property
            name="staticEntityRecordId"
            type="long"
            update="true"
            insert="true"
            column="STATIC_ENTITY_RECORD_ID"
            length="30"
        />

        <property
            name="linkStatus"
            type="string"
            update="true"
            insert="true"
            column="STATUS"
            length="10"
        />

        <property
            name="dynamicEntityRecordId"
            type="long"
            update="true"
            insert="true"
            column="DYNAMIC_ENTITY_RECORD_ID"
            length="30"
        />

        <property
            name="modifiedDate"
            type="date"
            update="true"
            insert="true"
            column="MODIFIED_DATE"
        />

        <property
            name="createdDate"
            type="date"
            update="true"
            insert="true"
            column="CREATED_DATE"
        />

        <property
            name="createdBy"
            type="string"
            update="true"
            insert="true"
            column="CREATED_BY"
            length="255"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-EntityMapRecord.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
