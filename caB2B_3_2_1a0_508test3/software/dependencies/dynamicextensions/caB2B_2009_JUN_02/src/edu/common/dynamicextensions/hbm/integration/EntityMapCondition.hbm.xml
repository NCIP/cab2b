<?xml version='1.0'?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class
        name="edu.common.dynamicextensions.domain.integration.EntityMapCondition"
        table="DYEXTN_ENTITY_MAP_CONDNS"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">DYEXTN_ENTITY_MAP_CONDN_SEQ</param>
            </generator>
        </id>

        <property
            name="staticRecordId"
            type="long"
            update="true"
            insert="true"
            column="STATIC_RECORD_ID"
            length="30"
        />

        <property
            name="typeId"
            type="long"
            update="true"
            insert="true"
            column="TYPE_ID"
            length="30"
        />

        <many-to-one
            name="formContext"
            class="edu.common.dynamicextensions.domain.integration.FormContext"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="FORM_CONTEXT_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-EntityMapCondition.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
