<?xml version='1.0'?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class
        name="edu.common.dynamicextensions.domain.PermissibleValue"
        table="DYEXTN_PERMISSIBLE_VALUE"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">DYEXTN_PERMISSIBLEVAL_SEQ</param>
            </generator>
        </id>

        <property
            name="description"
            type="string"
            update="true"
            insert="true"
            column="DESCRIPTION"
        />

        <set
            name="semanticPropertyCollection"
            lazy="false"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ABSTRACT_VALUE_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.SemanticProperty"
              />
        </set>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PermissibleValue.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="edu.common.dynamicextensions.domain.DoubleValue"
            table="DYEXTN_DOUBLE_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="double"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.ByteArrayValue"
            table="DYEXTN_BARR_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.BooleanValue"
            table="DYEXTN_BOOLEAN_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="boolean"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.DateValue"
            table="DYEXTN_DATE_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="timestamp"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.FloatValue"
            table="DYEXTN_FLOAT_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="float"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.LongValue"
            table="DYEXTN_LONG_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="long"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.StringValue"
            table="DYEXTN_STRING_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="string"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.ShortValue"
            table="DYEXTN_SHORT_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="short"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.IntegerValue"
            table="DYEXTN_INTEGER_CONCEPT_VALUE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="value"
            type="integer"
            update="true"
            insert="true"
            column="VALUE"
        />

        </joined-subclass>

    </class>

</hibernate-mapping>
