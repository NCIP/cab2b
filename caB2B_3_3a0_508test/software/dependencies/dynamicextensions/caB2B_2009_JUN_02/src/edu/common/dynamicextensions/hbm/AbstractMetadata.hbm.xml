<?xml version='1.0'?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class
        name="edu.common.dynamicextensions.domain.AbstractMetadata"
        table="DYEXTN_ABSTRACT_METADATA"
        dynamic-update="false"
        dynamic-insert="false"
    >
        <cache usage="read-write" />

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">DYEXTN_ABSTRACT_METADATA_SEQ</param>
            </generator>
        </id>

        <property
            name="createdDate"
            type="date"
            update="true"
            insert="true"
            column="CREATED_DATE"
        />

        <property
            name="description"
            type="string"
            update="true"
            insert="true"
            column="DESCRIPTION"
            length="1000"
        />

        <property
            name="lastUpdated"
            type="date"
            update="true"
            insert="true"
            column="LAST_UPDATED"
        />

        <property
            name="name"
            type="string"
            update="true"
            insert="true"
            column="NAME"
            length="1000"
        />

        <set
            name="taggedValueCollection"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ABSTRACT_METADATA_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.TaggedValue"
              />
        </set>

        <set
            name="semanticPropertyCollection"
            lazy="false"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ABSTRACT_METADATA_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.SemanticProperty"
              />
        </set>

        <property
            name="publicId"
            type="string"
            update="true"
            insert="true"
            column="PUBLIC_ID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-AbstractMetadata.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="edu.common.dynamicextensions.domain.AbstractAttribute"
            table="DYEXTN_ATTRIBUTE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />

        <set
            name="ruleCollection"
            table="DYEXTN_RULE"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ATTRIBUTE_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.validationrules.Rule"
              />
        </set>

        <many-to-one
            name="entity"
            class="edu.common.dynamicextensions.domain.Entity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="ENTIY_ID"
        />

        <joined-subclass
            name="edu.common.dynamicextensions.domain.Attribute"
            table="DYEXTN_PRIMITIVE_ATTRIBUTE"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />

        <set
            name="attributeTypeInformationCollection"
            table="DYEXTN_ATTRIBUTE_TYPE_INFO"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="PRIMITIVE_ATTRIBUTE_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.AttributeTypeInformation"
              />
        </set>

        <property
            name="isCollection"
            type="boolean"
            update="true"
            insert="true"
            column="IS_COLLECTION"
        />

        <property
            name="isIdentified"
            type="boolean"
            update="true"
            insert="true"
            column="IS_IDENTIFIED"
        />

        <property
            name="isPrimaryKey"
            type="boolean"
            update="true"
            insert="true"
            column="IS_PRIMARY_KEY"
        />

        <set
            name="columnPropertiesCollection"
            table="DYEXTN_COLUMN_PROPERTIES"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="PRIMITIVE_ATTRIBUTE_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.databaseproperties.ColumnProperties"
              />
        </set>

        <property
            name="isNullable"
            type="boolean"
            update="true"
            insert="true"
            column="IS_NULLABLE"
        />

        <set
            name="caDSRValueDomainInfoCollection"
            table="DYEXTN_CADSR_VALUE_DOMAIN_INFO"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="PRIMITIVE_ATTRIBUTE_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.CaDSRValueDomainInfo"
              />
        </set>

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.Association"
            table="DYEXTN_ASSOCIATION"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="direction"
            type="string"
            update="true"
            insert="true"
            column="DIRECTION"
        />

        <many-to-one
            name="targetEntity"
            class="edu.common.dynamicextensions.domain.Entity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="TARGET_ENTITY_ID"
        />

        <many-to-one
            name="sourceRole"
            class="edu.common.dynamicextensions.domain.Role"
            cascade="save-update"
            outer-join="auto"
            update="true"
            insert="true"
            column="SOURCE_ROLE_ID"
        />

        <many-to-one
            name="targetRole"
            class="edu.common.dynamicextensions.domain.Role"
            cascade="save-update"
            outer-join="auto"
            update="true"
            insert="true"
            column="TARGET_ROLE_ID"
        />

        <set
            name="constraintPropertiesCollection"
            table="DYEXTN_CONSTRAINT_PROPERTIES"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ASSOCIATION_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.databaseproperties.ConstraintProperties"
              />
        </set>

        <property
            name="isSystemGenerated"
            type="boolean"
            update="true"
            insert="true"
            column="IS_SYSTEM_GENERATED"
        />

        </joined-subclass>

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.EntityGroup"
            table="DYEXTN_ENTITY_GROUP"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />

        <set
            name="entityCollection"
            table="DYEXTN_ENTITY_GROUP_REL"
            lazy="false"
            inverse="true"
            cascade="none"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ENTITY_GROUP_ID"
              />

              <many-to-many
                  class="edu.common.dynamicextensions.domain.Entity"
                  column="ENTITY_ID"
                  outer-join="auto"
              />

        </set>

        <property
            name="longName"
            type="string"
            update="true"
            insert="true"
            column="LONG_NAME"
        />

        <property
            name="shortName"
            type="string"
            update="true"
            insert="true"
            column="SHORT_NAME"
        />

        <property
            name="version"
            type="string"
            update="true"
            insert="true"
            column="VERSION"
        />

        <set
            name="mainContainerCollection"
            table="DYEXTN_CONTAINER"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ENTITY_GROUP_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.userinterface.Container"
              />
        </set>

        <property
            name="isSystemGenerated"
            type="boolean"
            update="true"
            insert="true"
            column="IS_SYSTEM_GENERATED"
        />

        </joined-subclass>
        <joined-subclass
            name="edu.common.dynamicextensions.domain.Entity"
            table="DYEXTN_ENTITY"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="IDENTIFIER"
        />
        <property
            name="dataTableState"
            type="int"
            update="true"
            insert="true"
            column="DATA_TABLE_STATE"
        />

        <set
            name="entityGroupCollection"
            table="DYEXTN_ENTITY_GROUP_REL"
            lazy="false"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ENTITY_ID"
              />

              <many-to-many
                  class="edu.common.dynamicextensions.domain.EntityGroup"
                  column="ENTITY_GROUP_ID"
                  outer-join="auto"
              />

        </set>

        <set
            name="tablePropertiesCollection"
            table="DYEXTN_TABLE_PROPERTIES"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ENTITY_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.databaseproperties.TableProperties"
              />
        </set>

        <set
            name="abstractAttributeCollection"
            table="DYEXTN_ATTRIBUTE"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            sort="unsorted"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="ENTIY_ID"
              />

              <one-to-many
                  class="edu.common.dynamicextensions.domain.AbstractAttribute"
              />
        </set>

        <property
            name="abstract"
            type="boolean"
            update="true"
            insert="true"
            column="IS_ABSTRACT"
        />

        <many-to-one
            name="parentEntity"
            class="edu.common.dynamicextensions.domain.Entity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PARENT_ENTITY_ID"
        />

        <property
            name="inheritStrategy"
            type="int"
            update="true"
            insert="true"
            column="INHERITANCE_STRATEGY"
        />

        <property
            name="discriminatorColumn"
            type="string"
            update="true"
            insert="true"
            column="DISCRIMINATOR_COLUMN_NAME"
        />

        <property
            name="discriminatorValue"
            type="string"
            update="true"
            insert="true"
            column="DISCRIMINATOR_VALUE"
        />

        </joined-subclass>

    </class>

</hibernate-mapping>
