<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class
        name="edu.wustl.cab2b.server.category.multimodelcategory.MultiModelAttributeImpl"
        table="CAB2B_MMA"
        dynamic-update="false"
        dynamic-insert="false"
    >
        <cache usage="read-write" />

        <id
            name="id"
            column="ID"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">ID_SEQ</param>
            </generator>
        </id>

        <property
            name="visible"
            type="boolean"
            update="true"
            insert="true"
            column="ISVISIBLE"
            not-null="false"
        />

        <property
            name="attributeId"
            type="long"
            update="true"
            insert="true"
            column="DE_ATTRIBUTE_ID"
            length="30"
        />

        <property
            name="catAttrDelimitedIds"
            type="string"
            update="true"
            insert="true"
            column="CATEGORIAL_ATTRIBUTE_IDS"
            length="254"
            not-null="true"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-MultiModelAttributeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
