<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.wustl.cab2b.common.ejb.queryengine.querystatus">
  <class name="QueryStatus" table="QUERY_STATUS">
  	<id name="queryStatusId" type="long" column="ID">
  		<generator class="increment"></generator>
  	</id>
  	<property name="userId" type="long" column="USER_ID"></property>
  	<property name="queryId" type="long" column="QUERY_ID"></property>
  	<property name="parentId" type="long" column="PARENT_ID"></property>
  	<property name="conditions" type="string" column="CONDITIONS"></property>
  	<property name="startTime" type="timestamp" column="START_TIME"></property>
  	<property name="endTime" type="timestamp" column="END_TIME"></property>
  	<property name="status" type="character" column="STATUS"></property>
  	  	
  	<list name="urlStatus" table="URL_STATUS">
  		<key column="ID"></key>
  		<index type="integer" column="POSITION"></index>  		
  		<composite-element class="URLStatus">
  			<property name="queryId" type="long" column="QUERY_ID"></property>
  			<property name="urlId" type="long" column="URL_ID"></property>
  			<property name="status" type="character" column="STATUS"></property>
  			<property name="resultCount" type="long" column="RESULT_COUNT"></property>
  			<property name="message" type="string" column="MESSAGE"></property>
  			<property name="description" type="string" column="DESCRIPTION"></property>
  		</composite-element>
  	</list>
  	
  	<property name="resultCount" type="long" column="RESULT_COUNT"></property>
  	<property name="message" type="string" column="MESSAGE"></property>
  	<property name="description" type="string" column="DESCRIPTION"></property>
  </class>
</hibernate-mapping>