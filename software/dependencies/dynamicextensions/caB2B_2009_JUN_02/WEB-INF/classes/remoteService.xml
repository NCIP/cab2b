<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="remoteService" class="org.springframework.remoting.httpinvoker.HttpInvokerProxyFactoryBean">
    <property name="serviceUrl">
      <value>http://localhost:@@server.port@@/catissuecorecsm/http/remoteService</value>
    </property>
    <property name="serviceInterface">
      <value>gov.nih.nci.csm.sdk.application.common.RemoteSDKApplicationService</value>
    </property>
  </bean>
</beans>

