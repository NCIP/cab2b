<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
	<class name="edu.wustl.common.querysuite.queryobject.impl.JoinGraph"
		table="QUERY_JOIN_GRAPH" dynamic-update="false" dynamic-insert="false">
		<cache usage="read-write" />

		<id name="id" column="IDENTIFIER" type="long" length="30"
			unsaved-value="null">
			<generator class="native">
				<param name="sequence">QUERY_JOIN_GRAPH_SEQ</param>
			</generator>
		</id>

		<many-to-one name="graph"
			class="edu.wustl.common.hibernate.PersistentGraph" cascade="all"
			column="COMMONS_GRAPH_ID" />

	</class>

</hibernate-mapping>
