<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class
        name="edu.wustl.cab2b.common.user.ServiceURL"
        table="CAB2B_SERVICE_URL"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="urlId"
            column="URL_ID"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">URL_ID_SEQ</param>
            </generator>
        </id>

        <property
            name="entityGroupName"
            type="string"
            update="true"
            insert="true"
            column="ENTITY_GROUP_NAME"
            length="1024"
            not-null="true"
        />

        <property
            name="urlLocation"
            type="string"
            update="true"
            insert="true"
            column="URL"
            length="1024"
            not-null="true"
        />

        <property
            name="adminDefined"
            type="boolean"
            update="true"
            insert="true"
            column="ADMIN_DEFINED"
            not-null="true"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ServiceURL.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
